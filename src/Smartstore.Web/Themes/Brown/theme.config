<?xml version="1.0" encoding="utf-8" ?>
<Theme baseTheme="Yellow" title="Brown" description="'Brown' default theme" author="NIB-BANK/PMO" version="1.0">
    <Vars>
        <Var name="blue" type="Color">#1565c0</Var> <!-- Retained for non-brand elements -->
        <Var name="indigo" type="Color">#3f51b5</Var> <!-- Retained for non-brand elements -->
        <Var name="primary" type="Color">#f7b715</Var> <!-- Updated to brand primary -->
        <Var name="warning" type="Color">#9c653d</Var> <!-- Updated to brand secondary -->
        <Var name="link-color" type="Color">#c69210</Var> <!-- Darkened primary for contrast -->
        <Var name="inverse-footer" type="Boolean">true</Var>
        <Var name="footer-bg" type="Color">darken(#9c653d, 10%)</Var> <!-- Darkened secondary for footer -->
        <Var name="footer-inverse-color" type="Color">rgb(255, 255, 255)</Var>
        <Var name="footer-social-bg" type="Color">#f9c94d</Var> <!-- Lightened primary for social icons -->
        <Var name="header-bg" type="Color">#9c653d</Var> <!-- Secondary for header -->
        <Var name="inverse-menubar" type="Boolean">true</Var>
        <Var name="inverse-shopbar" type="Boolean">true</Var>
        <Var name="inverse-navbar" type="Boolean">true</Var>
        <Var name="megamenu-bg" type="Color">transparent</Var>
        <Var name="shopbar-label" type="Select#brand-colors">primary</Var>
        <Var name="costep-progress-color" type="Color">#f7b715</Var> <!-- Primary for checkout steps -->
        <Var name="offcanvas-cart-footer" type="Color">lighten(#9c653d, 7%)</Var> <!-- Lightened secondary -->
        <Var name="card-cap-bg" type="Color">#f9f1e6</Var> <!-- Neutral, complementary to brand colors -->

        <!-- Search -->
        <Var name="searchbox-bg-1" type="Color">mix(#f7b715, #fff, 20%)</Var> <!-- Light primary mix -->
        <Var name="searchbox-color-1" type="Color">mix(#9c653d, #000, 30%)</Var> <!-- Dark secondary mix -->
        <!--<Var name="searchbox-border-1" type="Color">rgba(255,255,255, 0)</Var>
        <Var name="searchbox-border-2" type="Color">rgba(255,255,255, 0)</Var>-->
    </Vars>
</Theme>